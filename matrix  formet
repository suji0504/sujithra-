public class MatrixExample {
    public static void main(String[] args) {
        int rows = 3;
        int cols = 3;

        int count = 1; 
        System.out.println("matrix");

        for (int i = 1; i <= rows; i++) {
            for (int j = 1; j <= cols; j++) {
                System.out.print(count + "\t");
                count++;
            }
            System.out.println(); 
        }
    }
}
