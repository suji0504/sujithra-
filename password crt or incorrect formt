import java.util.Scanner;

public class LoginCheck {
    public static void main(String[] args) {
        
        String correctUserId = "suji";
        String correctPassword = "0504";

        
        Scanner scanner = new Scanner(System.in);

       
        System.out.print("Enter User ID: ");
        String inputUserId = scanner.nextLine();

        System.out.print("Enter Password: ");
        String inputPassword = scanner.nextLine();

        
        if (inputUserId.equals(correctUserId) && inputPassword.equals(correctPassword)) {
            System.out.println("Login Successful: Correct ID and Password.");
        } else {
            System.out.println("Login Failed: Incorrect ID or Password.");
        }

        scanner.close();
    }
}
